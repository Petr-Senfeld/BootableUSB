<Window x:Class="USB_boot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:USB_boot"
        mc:Ignorable="d"
        Title="MainWindow" Height="320" Width="400">
    <Grid>
        <Button x:Name="ISO" Content="Vyber cestu k ISO" HorizontalAlignment="Left" Margin="24,230,0,0" VerticalAlignment="Top" Height="40" Width="108" Click="Button_Click_1"/>
        <Button Content="Vytvoř USB" HorizontalAlignment="Left" Margin="290,230,0,0" VerticalAlignment="Top" Height="40" Width="80" Click="Button_Create_USB"/>
        <ComboBox x:Name="USBbox" HorizontalAlignment="Center" VerticalAlignment="Top" Width="120" Margin="0,130,0,0"/>
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Margin="24,201,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="346"/>
        <TextBlock HorizontalAlignment="Left" Margin="24,180,0,0" Text="Cesta k ISO:" TextWrapping="Wrap" VerticalAlignment="Top" Width="101"/>

    </Grid>
</Window>
